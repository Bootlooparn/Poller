{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "target": "ES5",
        "jsx": "react",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
    },
    "exclude": ["node_modules"]
}